{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/smartmarkers-lib/**/*.{js,jsx,ts,tsx}",
      "!**/smartmarkers-lib/models/**",
      "!**/smartmarkers-lib/**/index.{js,ts}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "Kaushald4/vector-icons#master",
    "@react-native-community/datetimepicker": "2.2.2",
    "expo": "~37.0.12",
    "expo-font": "^8.1.1",
    "expo-linking": "^1.0.1",
    "fhirclient": "^2.3.1",
    "native-base": "^2.13.12",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-autocomplete-input": "^4.1.0",
    "react-native-screens": "~2.2.0",
    "react-native-url-polyfill": "^1.1.2",
    "react-native-web": "~0.11.7",
    "react-router-dom": "^5.1.2",
    "react-router-native": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.23",
    "@types/react-dom": "^16.9.6",
    "@types/react-native": "~0.61.17",
    "@types/react-native-autocomplete-input": "^4.0.1",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-router-native": "^5.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^6.8.0",
    "jest-expo": "^37.0.0",
    "prettier": "^2.0.4",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "~16.9.0",
    "typescript": "~3.8.3"
  },
  "private": true
